# Twitch API Credentials
# Get these from https://dev.twitch.tv/console/apps
TWITCH_CLIENT_ID=
TWITCH_CLIENT_SECRET=

# Google Sheets
# Path to service account JSON file used by gspread
GOOGLE_SHEETS_SERVICE_ACCOUNT_FILE=service_account.json

# TheTVDB
# Obtain an API key and a bearer token (JWT) from your TheTVDB account/app
THE_TVDB_API_KEY=
THE_TVDB_BEARER_TOKEN=

# TheSportsDB
# Public demo keys exist; set your own key if you have it
SPORTSDB_API_KEY=

# OpenWeatherMap
# Get your API key from https://openweathermap.org/api
# Required for weather calendar integration
OPENWEATHERMAP_API_KEY=